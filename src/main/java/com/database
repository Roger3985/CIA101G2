-- 創建 ProductPicture 表格
CREATE TABLE ProductPicture
(
    productPicNo         INT              NOT NULL   PRIMARY KEY   AUTO_INCREMENT,
    productNo            INT              NOT NULL,
    productPic           LONGBLOB,
    CONSTRAINT FK_ProductPicture_productNo foreign key(productNo) REFERENCES Product(productNo)
);


  -- 創建場地時段預約表格
CREATE TABLE StudioTimeBooking
(
  studioTimeNo INT NOT NULL PRIMARY KEY,
  studioOrdNo INT NULL,
  studioNo INT NULL,
  closeDate DATE NULL,
  closeTimeMorning TINYINT NOT NULL,
  closeTimeAfternoon TINYINT NOT NULL,
  closeTimeNight TINYINT NOT NULL,
  FOREIGN KEY (studioOrdNo) REFERENCES StudioOrder(studioOrdNo) ,
  FOREIGN KEY (studioNo) REFERENCES StudioInfo(studioNo)
);


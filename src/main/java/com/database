-- 創建最新消息表格
CREATE TABLE News(
	newsNo	INT NOT NUll PRIMARY KEY AUTO_INCREMENT,
	newsTitle	VARCHAR(45)	NOT NUll,
	newsContent	LONGTEXT NOT NUll,
	postTime	DATETIME NOT NUll
);


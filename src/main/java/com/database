-- 創建 ProductOrder 表格
CREATE TABLE ProductOrder
(
    productOrdNo         INT              Not Null PRIMARY KEY AUTO_INCREMENT,
    memNo          		 INT              Not Null,
    productByrName       VARCHAR(20)      Not Null,
    productByrPhone      VARCHAR(10),
    productByrEmail      VARCHAR(40)      Not Null,
    productRcvName       VARCHAR(20),
    productRcvPhone      VARCHAR (10),
    productTakeMethod    TINYINT          Not Null,
    productAddr          VARCHAR(100)     Null,
    productPayMethod     TINYINT          Not Null,
    productAllPrice      DECIMAL(6,0)     Not Null,
    coupNo               INT,
    productDisc          DECIMAL(6,0)     Not Null,
    productRealPrice     DECIMAL(6,0)     Not Null,
    productOrdTime       DATETIME         Not Null DEFAULT CURRENT_TIMESTAMP,
    productOrdStat       TINYINT          Not Null,
    productStat          TINYINT          Not Null,
   CONSTRAINT FK_ProductOrder_memNo foreign key(memNo) REFERENCES Member(memNo) ,

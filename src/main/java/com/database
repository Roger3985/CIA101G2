-- 創建客服紀錄圖片表格
CREATE TABLE ServicePicture(
	servicePicNo INT NOT NULL PRIMARY KEY,
	recordNo	INT NOT NULL,
	servicePic	LONGBLOB,
	CONSTRAINT FK_ServicePicture_recordNo FOREIGN KEY (recordNo) REFERENCES ServiceRecord(recordNo)
);


-- 創建商品表格
CREATE TABLE product
(
	productNo INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
    productCatNo INT NOT NULL,
    productName VARCHAR(40) NOT NULL,
    productInfo VARCHAR(1000) NOT NULL,
    productSize INT NOT NULL,
    productColor VARCHAR(10),
    productPrice DECIMAL(6,0) NOT NULL,
    productStat TINYINT NOT NULL,
    productSalQty INT NOT NULL,
    productComPeople INT NOT NULL,
    productComScore INT NOT NULL,
    FOREIGN KEY (productCatNo) REFERENCES ProductCategory(productCatNo)
);
